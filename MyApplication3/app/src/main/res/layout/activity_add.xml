<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Add">

    <TextView
        android:id="@+id/h1"
        android:layout_width="293dp"
        android:layout_height="56dp"
        android:text="Lägg till träningspass"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="40dp" />



    <Button
        android:id="@+id/menu_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="658dp" />

    <Button
        android:id="@+id/menu_calender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Calender"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="658dp" />

    <Button
        android:id="@+id/menu_graph"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Graph"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="658dp" />

    <Button
        android:id="@+id/menu_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Settings"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="658dp" />

    <Button
        android:id="@+id/saveNote"
        android:layout_width="88dp"
        android:layout_height="56dp"
        android:text="Save"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="592dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/notes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Anteckningar"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Övriga anteckningar:"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="191dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vilken typ av träning körde du?"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/add_condition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Kondition"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="311dp" />

    <Button
        android:id="@+id/add_strength"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Styrka"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="311dp" />

    <Button
        android:id="@+id/add_technique"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Teknik"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="311dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hur kändes träningspasset?"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hur är du fysiskt/psykiskt?"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="139dp" />

    <Button
        android:id="@+id/add_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add excersise"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="382dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="169dp"
        android:layout_height="43dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="380dp">

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/main_title"
        android:layout_width="171dp"
        android:layout_height="43dp"
        android:hint="Title"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="380dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="65dp"
        android:layout_height="57dp"
        android:text="Bra"
        tools:layout_editor_absoluteX="256dp"
        tools:layout_editor_absoluteY="103dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dåligt"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="105dp" />

</androidx.constraintlayout.widget.ConstraintLayout>